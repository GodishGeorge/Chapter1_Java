
/* Chapter 1.1 */
/* Write a that displays "Welcome to Java", "Welcome to Computer Science" and "Programming is fun"  */
public class chapter1 {

	public static void main(String[] args) {
		System.out.println("Welcome to Java!");
		System.out.println("Welcome to Computer Science!");
		System.out.println("Programming if fun!");
  }
}

/* Chapter 1.2 */
/* Write a program that displays "Welcome to Java" five times */

public class chapter1 {
	
	public static void main(String[] args) {
		System.out.println("Welcome to Java!");
		System.out.println("Welcome to Java!");
		System.out.println("Welcome to Java!");
		System.out.println("Welcome to Java!");
		System.out.println("Welcome to Java!");
	}

}

/* Chapter 1.3 */
/* Write a program that display "Java" with the letter "A" */

public class Chapter1 {

	public static void main(String[] args) {
		System.out.println("   A     A     A     A   A");
		System.out.println("   A    A A     A   A   A A");
		System.out.println("A  A   AAAAA     A A   AAAAA");
		System.out.println(" AA   A     A     A   A     A ");
	}

}

/* Chapter1.4 */
/* Write a program that displays a table */

public class Chapter1 {

	public static void main(String[] args) {
		System.out.println("a    a^2    a^3");
		System.out.println("1    1      1");
		System.out.println("2    4      8");
		System.out.println("3    9      27");
		System.out.println("4    16     64");
	}

}

/* Chapter1.5 */
/* Write a program that displays the result */

public class Chapter1 {

	public static void main(String[] args) {
		System.out.println((9.5 * 4.5 - 2.5 * 3) / (45.5 - 3.5));
	}

}

